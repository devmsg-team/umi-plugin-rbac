{
	"compilerOptions": {
		"outDir": "./dist",
		"rootDir": "./src",
		"declaration": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"skipLibCheck": true,
		"target": "es2019",
		"jsx": "react"
	},
	"include": ["src"]
}
